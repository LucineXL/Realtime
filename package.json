{
  "scripts": {
    "test1": "mocha tests/.setup.js tests/**/*-test.js --compilers css:mocha-compiler.js",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "build_dev": "rimraf ./dist && cross-env CODE_ENV=dev webpack --config ./build/webpack.prod.config.js --progress --colors",
    "build_qa": "rimraf ./dist && cross-env CODE_ENV=qa webpack --config ./build/webpack.prod.config.js --progress --colors",
    "build_online": "rimraf ./dist && cross-env CODE_ENV=online webpack --config ./build/webpack.prod.config.js --progress --colors",
    "analyze": "cross-env ANALYZE=1 npm run build_online",
    "dev": "cross-env CODE_ENV=dev node devServer"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/__mocks__/**",
      "!**/coverage/**",
      "!**/build/**",
      "!**/dist/**",
      "!**/api/**",
      "!**/network/**",
      "!**/assets/**",
      "!**/containers/**",
      "!**/middlewares/**",
      "!**/routes/**",
      "!**/selectors/**",
      "!**/mapping.js",
      "!**/store/**",
      "!**/utils/**",
      "!**/components/Router/**",
      "!**/reducers/index.js",
      "!**/src/index.jsx",
      "!**/devServer.js",
      "!**/postcss.config.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    }
  },
  "dependencies": {
    "antd": "3.6.6",
    "axios": "0.18.0",
    "axios-mock-adapter": "1.14.1",
    "babel-polyfill": "6.26.0",
    "echarts": "^4.2.1",
    "echarts-for-react": "^2.0.15-beta.0",
    "enquire-js": "0.1.3",
    "fastclick": "1.0.6",
    "history": "4.7.2",
    "lodash-decorators": "5.0.0",
    "path-to-regexp": "2.2.0",
    "prop-types": "15.6.1",
    "rc-drawer-menu": "0.5.7",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-hot-loader": "4.0.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "3.7.2",
    "redux-devtools": "3.4.1",
    "redux-devtools-dock-monitor": "1.1.3",
    "redux-devtools-log-monitor": "1.4.0",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "7.2.6",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.3",
    "babel-jest": "22.4.3",
    "babel-loader": "7.1.4",
    "babel-plugin-import": "1.6.7",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "babel-runtime": "6.26.0",
    "cross-env": "5.1.4",
    "css-loader": "0.28.11",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "2.10.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "express": "4.16.3",
    "file-loader": "1.1.11",
    "git-revision-webpack-plugin": "2.5.1",
    "html-webpack-plugin": "3.1.0",
    "http-proxy-middleware": "0.18.0",
    "identity-obj-proxy": "3.0.0",
    "inline-manifest-webpack-plugin": "4.0.1",
    "jest": "22.4.3",
    "less": "2.7.3",
    "less-loader": "4.1.0",
    "mini-css-extract-plugin": "0.4.0",
    "postcss-loader": "2.1.3",
    "postcss-px2rem": "0.3.0",
    "react-test-renderer": "16.2.0",
    "rimraf": "2.6.2",
    "style-loader": "0.20.3",
    "uglifyjs-webpack-plugin": "1.2.4",
    "url-loader": "1.0.1",
    "webpack": "4.5.0",
    "webpack-bundle-analyzer": "2.11.1",
    "webpack-cli": "2.0.14",
    "webpack-dev-middleware": "3.1.0",
    "webpack-hot-middleware": "2.21.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie >= 9",
    "iOS >= 8",
    "Android >= 4"
  ],
  "theme": {
    "@primary-color": "#46b699",
    "@btn-border-radius-base": "20px",
    "@btn-border-radius-sm": "10px",
    "@icon-url": "'../~assets/fonts/iconfont'"
  }
}
