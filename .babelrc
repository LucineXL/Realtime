{
  "presets": [[
    "env", {
      "targets": {
        "browsers": [
          "> 1%",
          "last 2 versions",
          "ie >= 9",
          "iOS >= 8",
          "Android >= 4"
        ]
      },
      "modules": false,
      "useBuiltIns": true,
      "debug": false
    }
  ], "react", "stage-0"],
  "plugins": ["react-hot-loader/babel", "transform-decorators-legacy", ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": true }]]
}
